	"scripts": {
		"ngc": "ngc -p ./tsconfig.json",
		"start": "npm run ngc && webpack-dev-server --config config/webpack.dev.config.js --progress --hot --inline --watch",
		"start:prod": "npm run build-prod && lite-server -c config/bs-config.js",
		"build:dev": "rm -rf ./dist && npm run ngc && webpack --config config/webpack.dev.config.js",
		"build:prod": "rm -rf ./dist && npm run ngc && webpack --config config/webpack.prod.config.js"
	},
	"private": true,
