{
	"name": "goci",
	"version": "1.0.0",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/prsolucoes/goci.git"
	},
	"author": {
		"name": "Paulo Coutinho",
		"email": "paulo@prsolucoes.com"
	},
	"scripts": {
		"start": "npm run ngc && webpack-dev-server --config webpack.dev.config.js --progress --colors --hot --inline --host 0.0.0.0 --port 9000",
		"postinstall": "typings install",
		"tsc": "tsc",
		"tsc:w": "tsc -w",
		"typings": "typings",
		"webpack": "npm run ngc && webpack --config webpack.dev.config.js --progress --colors",
		"webpack-dev-server": "npm run ngc && webpack-dev-server --config webpack.dev.config.js --progress --colors --host 0.0.0.0",
		"webpack-watch": "npm run ngc && webpack --config webpack.dev.config.js --progress --colors --watch",
		"webpack-watch-dev": "npm run ngc && webpack-dev-server --config webpack.dev.config.js --progress --colors --watch --host 0.0.0.0",
		"build": "npm run ngc && webpack --config webpack.prod.config.js --progress --profile --bail && rm -rf ../resources/web-app && cp -r dist ../resources/web-app",
		"ngc": "ngc -p ./tsconfig.json"
	},
	"private": true,
	"dependencies": {
		"@angular/common": "2.0.1",
		"@angular/compiler": "2.0.1",
		"@angular/compiler-cli": "^0.6.2",
		"@angular/core": "2.0.1",
		"@angular/forms": "2.0.1",
		"@angular/http": "2.0.1",
		"@angular/platform-browser": "2.0.1",
		"@angular/platform-browser-dynamic": "2.0.1",
		"@angular/router": "3.0.1",
		"@angular/upgrade": "2.0.1",
		"@angular/platform-server": "2.0.1",
		"bootstrap": "3.3.7",
		"chart.js": "2.3.0",
		"core-js": "2.4.1",
		"jquery": "2.2.4",
		"ng2-charts": "1.3.0",
		"reflect-metadata": "0.1.8",
		"rxjs": "5.0.0-beta.12",
		"toastr": "2.1.2",
		"zone.js": "0.6.25"
	},
	"devDependencies": {
		"angular2-template-loader": "0.5.0",
		"concurrently": "2.2.0",
		"css-loader": "0.25.0",
		"extract-text-webpack-plugin": "1.0.1",
		"favicons-webpack-plugin": "0.0.7",
		"file-loader": "0.9.0",
		"html-loader": "0.4.4",
		"html-webpack-plugin": "2.22.0",
		"lite-server": "2.2.2",
		"null-loader": "0.1.1",
		"raw-loader": "0.5.1",
		"style-loader": "0.13.1",
		"typescript": "2.0.3",
		"typings": "1.4.0",
		"url-loader": "0.5.7",
		"webpack": "1.13.2",
		"webpack-dev-server": "1.16.1",
		"awesome-typescript-loader": "^2.2.4"
	}
}
